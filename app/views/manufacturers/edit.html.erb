<h1>Editing manufacturer</h1>

<% form_for(@manufacturer) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :logo %><br />
    <%= f.text_field :logo %>
  </p>
  <p>
    <%= f.label :is_active %><br />
    <%= f.text_field :is_active %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @manufacturer %> |
<%= link_to 'Back', manufacturers_path %>